<p class="text-center">
  <strong>Logo: </strong>
  <img src="<%= @choir.image_url(:thumb).to_s%>">
</p>

<p>
  <strong>Name:</strong>
  <%= @choir.name %>
</p>

<p>
  <strong>Website:</strong>
  <%= @choir.website %>
</p>

<p>
  <strong>Kategorie:</strong>
  <%= @choir.category.name if @choir.category.present?%>
</p>

<p>
  <strong>Rehearsal location name:</strong>
  <%= @choir.location_name %>
</p>

<p>
  <strong>Street name:</strong>
  <%= @choir.street_name %>
</p>

<p>
  <strong>House no:</strong>
  <%= @choir.house_no %>
</p>

<p>
  <strong>Zipcode:</strong>
  <%= @choir.zipcode %>
</p>

<p>
  <strong>City:</strong>
  <%= @choir.city %>
</p>

<p>
  <strong>Country:</strong>
  <%= @choir.country %>
</p>

<p>
  <strong>Latitude:</strong>
  <%= @choir.latitude %>
</p>

<p>
  <strong>Longitude:</strong>
  <%= @choir.longitude %>
</p>

<%= link_to 'Edit', edit_choir_path(@choir) %> |
<%= link_to 'Back', choirs_path %>
